{
  "name": "discours-editor-widget",
  "description": "Discours.io editor widget",
  "version": "0.0.1",
  "scripts": {
    "dev": "node esbuild.config.js",
    "build": "cross-env NODE_ENV='production' node esbuild.config.js",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "fix": "eslint --fix src/**/*"
  },
  "engines": {
    "pnpm": "^6"
  },
  "dependencies": {
    "@vercel/node": "^1.12.1",
    "faunadb": "^4.4.1"
  },
  "devDependencies": {
    "@tsconfig/svelte": "^2.0.1",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "cross-env": "^7.0.3",
    "derver": "^0.4.20",
    "dotenv": "^10.0.0",
    "esbuild": "^0.12.29",
    "esbuild-svelte": "^0.5.7",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.8",
    "svelte-preprocess": "^4.9.8",
    "svelte-preprocess-esbuild": "^2.0.0",
    "svelte-table": "0.2.0",
    "svelte-windicss-preprocess": "^4.1.4",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4",
    "y-indexeddb": "^9.0.6",
    "y-protocols": "^1.0.5",
    "y-webrtc": "^10.2.0",
    "yjs": "^13.5.17"
  },
  "type": "module"
}
