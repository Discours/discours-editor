    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.4",
    "svelte-preprocess-esbuild": "^2.0.0",
    "svelte-table": "^0.1.18",
    "svelte2tsx": "^0.4.6",
    "tailwindcss": "^2.2.15",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "y-indexeddb": "^9.0.6",
    "y-protocols": "^1.0.5",
    "y-webrtc": "^10.2.0",
    "yjs": "^13.5.12"
